{
	"name": "webtex",
	"version": "2.0.0",
	"description": "Renders Inline & Block LaTeX On Sites That Don't Support It with KaTeX and intelligent fallback.",
	"private": true,
	"scripts": {
		"watch": "node ./node_modules/webpack/bin/webpack.js --mode=development --watch --config config/webpack.config.js",
		"build": "node ./node_modules/webpack/bin/webpack.js --mode=production --config config/webpack.config.js",
		"check": "npx @biomejs/biome check --write --max-diagnostics=1000",
		"dev": "node dev-server.js",
		"start": "npm run build && npm run dev"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"copy-webpack-plugin": "^13.0.0",
		"css-loader": "^7.1.2",
		"express": "^4.18.2",
		"file-loader": "^6.2.0",
		"mini-css-extract-plugin": "^2.9.2",
		"size-plugin": "^1.2.0",
		"webpack": "^5.101.0",
		"webpack-cli": "^6.0.1",
		"webpack-dev-middleware": "^6.1.1",
		"webpack-merge": "^6.0.1"
	},
	"dependencies": {
		"katex": "^0.16.22",
		"open": "^10.2.0"
	}
}
